{
    "name": "bexio-chrome-extension",
    "version": "0.11.0",
    "description": "This extension adds saving and loading of form data within bexios time tracking page.",
    "scripts": {
        "devRelease": "npm run version:patch && npm run buildProject",
        "devRelease:sidePanelOnly": "npm run version:patch && npm run buildProject -- -IgnoreExtension",
        "devRelease:extensionOnly": "npm run version:patch && npm run buildProject -- -IgnoreSidePanel",
        "buildProject": "powershell -File build.ps1",
        "createPackage": "npm run buildProject -- -createPackage",
        "cleanup": "powershell \"Remove-Item -Path dist -Recurse -Force; Remove-Item -Path unpacked -Recurse -Force;\"",
        "version:patch": "npm --no-git-tag-version version patch",
        "version:minor": "npm --no-git-tag-version version minor",
        "version:major": "npm --no-git-tag-version version major",
        "postversion": "node updateManifest.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/SoulcodeAgency/bexio-chrome-extension.git"
    },
    "keywords": [
        "bexio",
        "time",
        "tracking",
        "soulcode",
        "chrome",
        "extension"
    ],
    "author": "Fabian Gander",
    "license": "AGPL-3.0",
    "bugs": {
        "url": "https://github.com/SoulcodeAgency/bexio-chrome-extension/issues"
    },
    "homepage": "https://github.com/SoulcodeAgency/bexio-chrome-extension#readme",
    "devDependencies": {
        "fs-extra": "^11.1.1"
    },
    "workspaces": [
        "bexio-chrome-extension",
        "sidePanel-import"
    ]
}
